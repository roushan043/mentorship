

#include<bits/stdc++.h>

using namespace std;

int main()
{ int n,y=0,p, rem, count=0,rous;
  cout<<"input a no";
  cin>>n;
   p=n;
   do{
       n=n/10;
       count++;
     } while(n!=0);
rous=p;
  while(rous!=0){
       rem=rous%10;
       y=y+pow(rem,count);
       rous=rous/10;
  }
 if(p==y)
cout<<"armstrong no";
else
cout<<"not a armstrong no";
return 0;
}

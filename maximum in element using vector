

#include <bits/stdc++.h>

using namespace std;

int main()
{
    vector<int>n;
    int a,p;
    cout<<"Enter number of elements in vector : ";
    cin>>a;
    for(int i=0;i<a;i++){
        int e;
        cin>>e;
        n.push_back(e);
        
    }
    
    int max=0;
    for(int j=0; j<n.size(); j++){
        if(max<n[j]){
            max=n[j];
        }
    }
    cout<<max;
    

    return 0;
}

#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    cout<<"even no";
    for(int i=0; i<=n; i++)
        {
          if(i%2==0)
          cout<<i<<endl;
        }
        cout<<"odd no";
        for(int i=0; i<=n; i++)
          {
              if(i%2!=0)
              cout<<i<<endl;
          }
        

    return 0;
}

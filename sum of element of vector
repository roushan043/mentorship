#include <bits/stdc++.h>

using namespace std;

int main()
{
    int s=0;
    vector<int>a;
    cout<<"input a elements";
     int p;
     cin>>p;
    for(int j=0; j<p; j++){
       int z;
       cin>>z;
       a.push_back(z);
        }
    
  for(int i=0; i<a.size(); i++)
  {
      s=s+a[i];
      
  }
  cout<<s;
    
    
    

    return 0;
}
